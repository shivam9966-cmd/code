import pandas as pd
from sqlalchemy import create_engine


engine = create_engine(
    "postgresql+psycopg2://postgres:your_password@localhost:5432/your_database"
)


df = pd.read_csv("one_month_data.csv")

df.to_sql(
    "one_month_data",     # table name
    engine,
    if_exists="replace",
    index=False
)

print("one_month_data uploaded successfully")
