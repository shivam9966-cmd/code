revenue_variance_df = compute_variance(granular, "Revenue")
opex_variance_df    = compute_variance(granular, "Operating Expenses")
ebitda_variance_df  = compute_ebitda_variance(granular)


revenue_variance_df.show(truncate=False)
opex_variance_df.show(truncate=False)
ebitda_variance_df.show(truncate=False)
